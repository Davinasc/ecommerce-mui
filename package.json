{
  "name": "ecommerce-mui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "Davi Nascimento",
  "repository": {
    "type": "git",
    "url": "https://github.com/Davinasc/ecommerce-mui"
  },
  "scripts": {
    "dev": "vite --mode=development --host",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint --fix --cache --env-info src/**/*.{js,ts,tsx}",
    "prettify": "prettier --write 'src/**/*.{js,mjs,ts,tsx,json,css,md,html}'",
    "upgrade": "yarn upgrade-interactive",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky",
    "pre-commit": "",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:ui": "vitest --ui",
    "test:pre-commit": "vitest related --run --environment=jsdom",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.0",
    "react-router-dom": "^7.5.1",
    "swr": "^2.3.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@hookform/devtools": "^4.4.0",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "msw": "^2.7.5",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
